// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model ClientOauth {
  id           String   @id @default(uuid())
  clientId     String  @unique
  clientSecret String
  redirectUris String[]
  grants       Grant[]
}

enum Grant {
  client_credentials
  authorization_code
  refresh_token
  password
}
